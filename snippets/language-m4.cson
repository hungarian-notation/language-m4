'.source.m4':
  'm4 define':
    'prefix': 'm4_define'
    'body': 'm4_define([${1:name}], [${2:definition}])${3}'
  'm4 ifelse':
    'prefix': 'm4_ifelse'
    'body': 'm4_ifelse([${1:value}], [${2:test}], [${3:equal}], [${4:not-equal}])${5}'
  'm4sugar if':
    'prefix': 'm4_if'
    'body': 'm4_if([${1:value}], [${2:test}], [${3:equal}], [${4:not-equal}])${5}'
